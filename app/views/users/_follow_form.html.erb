<% if current_user?(user) %>
	<%= render 'users/edit_profile'%>
<% else %>
  <div id="follow_form">
  <% if current_user.following?(user) %>
    <%= render :partial => "users/unfollow", object: user, as: :user %>
  <% else %>
    <%= render :partial => "users/follow", object: user, as: :user %>
  <% end %>
  </div>
<% end %>